        "start": "webpack serve --mode development --port 3000 --hot",
        "watch": "webpack --mode production --watch",
        "watch:dev": "webpack --mode development --watch",
        "watch:prod": "webpack --mode production --watch",
        "watch:css": "tailwindcss -i ./src/**/**.css -o ./public/styles.css --watch",
        "build": "webpack --mode production",
        "build:dev": "webpack --mode development",
        "build:css": "tailwindcss -i ./src/**/**.css -o ./public/styles.css",
        "scrape": "node ../imgnx-scraper/index.js && cp -r ../imgnx-scraper/dist/** ./src/ && tailwindcss -i ./src/**/**.css -o ./dist/output.css --minify"
